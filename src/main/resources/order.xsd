                                    <xs:sequence>
                                        <xs:element name="FirstName" type="xs:string" minOccurs="0" />
                                        <xs:element name="LastName" minOccurs="0">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:maxLength value="40" />
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="CompanyName" minOccurs="0" />
